{
  "name": "@corentints/tanstack-router-sitemap",
  "version": "1.0.0",
  "description": "Generate XML sitemaps from TanStack Router route definitions",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "preview": "vite preview",
    "demo": "npm run build && node demo.js",
    "demo:plugin": "npm run build && node plugin-demo.js"
  },
  "keywords": [
    "tanstack",
    "router",
    "sitemap",
    "seo",
    "xml",
    "typescript",
    "vite"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "dependencies": {
    "@tanstack/router-core": "^1.121.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@tanstack/router-core": ">=1.121.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Corentints/tanstack-router-sitemap.git"
  },
  "bugs": {
    "url": "https://github.com/Corentints/tanstack-router-sitemap/issues"
  },
  "homepage": "https://github.com/Corentints/tanstack-router-sitemap#readme"
}
